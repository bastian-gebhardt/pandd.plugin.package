<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE project>
<project name="pandd_packaging_lib" basedir="." xmlns:artifact="antlib:org.apache.maven.artifact.ant">
    <description>
    	builds a archiv for using as lib
    </description>

	<property name="file-type" value="jar"/>
	
	<target name="_package" >
		<zip destfile="dist/${package_file}.jar">
			<fileset id="package_fileset" dir="src/" />
		</zip>	
	</target>
	
</project>